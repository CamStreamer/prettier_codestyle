{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "trailingComma": "es5",
  "endOfLine": "lf",
  "tabWidth": 4,
  "semi": true,
  "quoteProps": "consistent",
  "printWidth": 120,
  "overrides": [
    {
      "files": ["*.yml", "*.yaml"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
